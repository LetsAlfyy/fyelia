{
  "version": 2,
  "builds": [
    {
      "src": "api/data.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/data",
      "methods": ["GET", "POST", "DELETE", "OPTIONS"],
      "dest": "/api/data.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
